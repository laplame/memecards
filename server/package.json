{
  "name": "audio-server",
  "version": "1.0.0",
  "description": "Backend server for audio upload and processing",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "generate-pages": "tsx scripts/generate-100-pages.ts",
    "seed-stores": "tsx scripts/seed-stores.ts",
    "create-test-pages": "tsx scripts/create-test-pages.ts",
    "update-pages": "tsx scripts/update-existing-pages.ts",
    "generate-unused": "tsx scripts/generate-10-unused-pages.ts",
    "check-env": "tsx scripts/check-env.ts"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fluent-ffmpeg": "^2.1.2",
    "form-data": "^4.0.0",
    "mongoose": "^8.0.3",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "qrcode": "^1.5.4",
    "sharp": "^0.34.5",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.6",
    "@types/qrcode": "^1.5.6",
    "@types/uuid": "^9.0.7",
    "tsx": "^4.7.0",
    "typescript": "^5.5.3"
  }
}
